package com.linkedlist;

 



public class LinkedlList {
	class Node {
		
		 int data;
		 Node next;
		
		public Node(int data)
		{
			this.data=data;
			next=null;
		}

		
	}

	Node head;

	public Node getHead() {
		return head;
	}

	public LinkedlList()
	{
		head=null;
	}
	
	public void insert(int data)
	{
		Node newNode=new Node(data);
		
		if(head==null)
		{
			head=newNode;
		}
		
		Node temp=head;
		while(temp.next!=null)
		{
			temp=temp.next;
		}
		temp.next=newnode;
		
	}
	
	
}
