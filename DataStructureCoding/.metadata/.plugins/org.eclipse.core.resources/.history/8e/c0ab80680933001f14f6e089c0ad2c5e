package com.arrayList;


public class Stack {
	
	int [] list;
	int size;
	int top;
	public Stack( int size) {
		super();
	
		this.size = size;
		this.top = -1;
		list=new int[size];
	}
	public boolean isEmpty() {
		
		return top==-1;
	}
	public boolean isFull()
	{
		
		return top==(size-1);
	}
	public boolean push(int data) {
		if(isFull()) {
			return false;
			
		}
		list[++top]=data;
		
		
		return true;
	}
	
	public void pop()
	{
		if(isEmpty())
		{
			return null;
		}
		
		
		
	}
	
	
	
	
	
	
	
}
